{
  "name": "jpjuliao/md2elementor",
  "description": "Convert Markdown to Elementor layouts",
  "type": "wordpress-plugin",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "JPJuliao\\MD2Elementor\\": "includes/"
    }
  },
  "require": {
    "erusev/parsedown": "^1.7",
    "php": ">=7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.13"
  },
  "autoload-dev": {
    "psr-4": {
      "JPJuliao\\MD2Elementor\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --testdox",
    "cs": "phpcs",
    "cs-fix": "phpcbf",
    "check": [
      "@cs",
      "@test"
    ]
  }
}
